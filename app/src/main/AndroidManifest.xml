<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scrappers.covid19_guide">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/coronavirus_3d_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/coronavirus_3d_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.scrappers.covid19_guide.COVID19_Maps"></activity>
        <activity android:name="com.scrappers.covid19_guide.lists" />
        <activity android:name="com.scrappers.covid19_guide.aboutActivity"/>
        <activity android:name="com.scrappers.covid19_guide.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>